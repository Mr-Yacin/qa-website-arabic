{
	"version": 3,
	"routes": [
		{
			"src": "/_server-islands/([^/]+?)/",
			"headers": {
				"Location": "/_server-islands/$1"
			},
			"status": 308
		},
		{
			"src": "/q/([^/]+?)/",
			"headers": {
				"Location": "/q/$1"
			},
			"status": 308
		},
		{
			"src": "/tags/([^/]+?)/",
			"headers": {
				"Location": "/tags/$1"
			},
			"status": 308
		},
		{
			"src": "/tags/",
			"headers": {
				"Location": "/tags"
			},
			"status": 308
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_server-islands\\/([^/]+?)$",
			"dest": "_render"
		},
		{
			"src": "^\\/_image$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/avg$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/rate$",
			"dest": "_render"
		},
		{
			"src": "^\\/q\\/([^/]+?)$",
			"dest": "_render"
		},
		{
			"src": "^\\/$",
			"dest": "_render"
		}
	]
}